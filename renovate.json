{
  "extends": [
    "config:base"
  ],
  "groupName": [
    "all"
  ],
  "packageRules": [
    {
      "packageNames": [
        "snyk"
      ],
      "extends": [
        "schedule:monthly"
      ]
    },
    {
      "updateTypes": [
        "minor",
        "patch",
        "pin",
        "digest",
        "chore(deps)",
        "fix(deps)"
      ],
      "automerge": true
    }
  ]
}
